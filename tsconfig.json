{
  "compilerOptions": {
    "target": "es2016",
    "types": [
      "node"
    ],
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "outDir": "./out",
    "rootDir": "./",
    "skipLibCheck": true,
    "moduleResolution": "nodenext"
  },
  "include": [
    "*/**/*"
  ],
  "exclude": [
    "./out/**"
  ]
}
